<UserControl
    x:Class="PrimeDocs_Desktop.UI.Components.Home"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PrimeDocs_Desktop.UI.Components"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:Height="35"
    d:Width="35"
    mc:Ignorable="d">
    <Grid>
        <Button
            MinWidth="35"
            MinHeight="35"
            MaxWidth="35"
            MaxHeight="35"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            BorderBrush="{x:Null}"
            Click="btInitialWindowHome_Click"
            Focusable="False"
            Style="{StaticResource WindowControlHomeNormalButtonStyle}"
            ToolTip="Início">
            <Grid>
                <Image
                    MinWidth="30"
                    MinHeight="30"
                    MaxWidth="30"
                    MaxHeight="30"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Source="/Resources/Icons/InitialWindow/PrimeDocsBar.png" />
            </Grid>
        </Button>
    </Grid>
</UserControl>
