<UserControl x:Class="PrimeDocs_Desktop.UI.Components.Config"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrimeDocs_Desktop.UI.Components"
             mc:Ignorable="d" >
    <Grid>
        <Button Focusable="False" Style="{StaticResource ProfileConfigButtonStyle}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Click="btInitialWindowConfig_Click">
            <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="/Resources/Icons/InitialWindow/config.png" Stretch="Fill"/>
        </Button>
    </Grid>
</UserControl>
